{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:default",
    "core:event:default",
    "process:allow-exit"
  ]
}